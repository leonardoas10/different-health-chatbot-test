  "description": "API rest for Different Health",
  "main": "dist/server.js",
  "scripts": {
    "start": "node src/server.ts",
    "clean": "rimraf dist",
    "dev": "NODE_ENV=development nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
